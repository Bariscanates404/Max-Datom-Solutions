(d/q '[:find  ?user-name (count ?posts)
       :where [?user :user/id #uuid "1B341635-BE22-4ACC-AE5B-D81D8B1B7678"]
              [?user :user/first+last-name ?user-name]
              [?posts :post/author ?user]] db)
